<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.perfume.mybatis.Re_Q_ABoardMapper">
	<insert id="re_Q_ABoard" parameterType="com.perfume.RE_BoardDTO">
	
		<selectKey resultType = "int" keyProperty="boaed_re_ref" order="BEFORE">
			select ifnull(max(boaed_re_ref),0) +1
				form tbl_reboard
		</selectKey>
	</insert>
	
	
</mapper>